<?xml version="1.0" encoding="UTF-8"?>
<databaseChangeLog xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
                   xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                   xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog
                   https://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-latest.xsd">

    <changeSet id="1" author="ysfky">

        <createTable tableName="foods_photos">
            <column name="foods_id" type="${uuid_type}"/>
            <column name="photos_id" type="${uuid_type}"/>
        </createTable>

        <createTable tableName="food_photos">
            <column name="id" type="${uuid_type}"/>
            <column name="keys" type="varchar(255)"/>
            <column name="food_id" type="${uuid_type}"/>
        </createTable>


    </changeSet>
</databaseChangeLog>
